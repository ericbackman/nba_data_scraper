   "source": [
    "# Compile all seasons post 2001 into single dataframe\n",
    "options = Options()\n",
    "options.page_load_strategy = 'eager' # Faster load so it does not wait for video ads to render\n",
    "team = \"TOR\"\n",
    "master_df = pd.DataFrame()\n",
    "\n",
    "for i in range(2001, 2024):\n",
    "    driver = webdriver.Chrome(options=options)\n",
    "    df_raw = getScheduleResultsDF(driver, team, str(i))\n",
    "    driver.close()\n",
    "    df_clean = cleanScheduleResultsDF(df_raw, team)\n",
    "    master_df = pd.concat([master_df, df_clean])\n",
    "\n",
    "master_df"
   ]
